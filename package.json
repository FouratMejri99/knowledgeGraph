{
  "name": "webview-frontend-extension",
  "displayName": "Webview Frontend Extension",
  "description": "VSCode extension that opens your frontend in a webview",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:webviewFrontend.openWebview"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vscode": "^1.1.55"
  },
  "contributes": {
    "commands": [
      {
        "command": "webviewFrontend.openWebview",
        "title": "Open Frontend Webview"
      }
    ]
  }
}
